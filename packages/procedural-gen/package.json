{
  "name": "@jbcom/procedural-gen",
  "version": "0.1.0",
  "description": "Unified procedural generation library for react-three-fiber - SDF, marching cubes, instancing, and raymarching under one roof",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./shaders": {
      "types": "./dist/shaders/index.d.ts",
      "import": "./dist/shaders/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "procedural-generation",
    "three.js",
    "threejs",
    "react-three-fiber",
    "r3f",
    "webgl",
    "sdf",
    "signed-distance-field",
    "marching-cubes",
    "gpu-instancing",
    "raymarching",
    "volumetric",
    "terrain",
    "water",
    "fog",
    "vegetation",
    "shaders",
    "glsl",
    "pbr"
  ],
  "author": "jbcom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/procedural-gen"
  },
  "peerDependencies": {
    "@react-three/drei": ">=9.0.0",
    "@react-three/fiber": ">=8.0.0",
    "react": ">=18.0.0",
    "three": ">=0.150.0"
  },
  "devDependencies": {
    "@react-three/fiber": "^8.15.0",
    "@types/react": "^18.2.0",
    "@types/three": "^0.160.0",
    "@vitest/coverage-v8": "^2.1.0",
    "react": "^18.2.0",
    "three": "^0.160.0",
    "typescript": "^5.3.0",
    "vitest": "^2.1.0"
  }
}
