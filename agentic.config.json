{
  "$schema": "https://agentic-control.dev/schema/config.json",
  "tokens": {
    "organizations": {
      "jbcom": {
        "name": "jbcom",
        "tokenEnvVar": "GITHUB_JBCOM_TOKEN",
        "defaultBranch": "main",
        "isEnterprise": false
      }
    },
    "defaultTokenEnvVar": "GITHUB_TOKEN",
    "prReviewTokenEnvVar": "GITHUB_JBCOM_TOKEN"
  },
  "defaultModel": "claude-sonnet-4-5-20250929",
  "defaultRepository": "jbcom/jbcom-oss-ecosystem",
  "logLevel": "info",
  "triage": {
    "provider": "anthropic",
    "model": "claude-sonnet-4-20250514",
    "apiKeyEnvVar": "ANTHROPIC_API_KEY"
  },
  "mcp": {
    "cursor": {
      "enabled": true,
      "tokenEnvVar": "CURSOR_API_KEY",
      "mode": "stdio",
      "command": "npx",
      "args": ["-y", "cursor-mcp-server"]
    },
    "github": {
      "enabled": true,
      "tokenEnvVar": "GITHUB_JBCOM_TOKEN",
      "tokenEnvVarFallbacks": ["GITHUB_TOKEN"],
      "mode": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"]
    },
    "context7": {
      "enabled": true,
      "tokenEnvVar": "CONTEXT7_API_KEY",
      "mode": "stdio",
      "command": "npx",
      "args": ["-y", "@anthropic/context7-mcp"]
    }
  }
}
